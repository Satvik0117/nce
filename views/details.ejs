<!DOCTYPE html>
<html lang="zxx">

<head>
    <meta charset="UTF-8">
    <meta name="description" content="Certification for fitness trainers: ultimate trainer certification, nutritional science certification and supplements science certification">
    <meta name="keywords" content="Nepal fitness, trainer certification, trainer course, fitness degree, gym trainer">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Nepal Council of Exercise | Trainer Certification</title>

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=PT+Sans:400,700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Oswald:400,500,600,700&display=swap" rel="stylesheet">

    <!-- Css Styles -->
    <link rel="stylesheet" href="css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="css/flaticon.css" type="text/css">
    <link rel="stylesheet" href="css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="css/magnific-popup.css" type="text/css">
    <link rel="stylesheet" href="css/slicknav.min.css" type="text/css">
    <link rel="stylesheet" href="css/style.css" type="text/css">

    <script src="js/jquery-3.2.1.min.js"></script>
</head>

<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>
    <!-- Header Section Begin -->
        <header class="header-section">
        <div class="container-fluid">
            <div class="inner-header">
                <div class="row">
                    <div class="col-md-3">
                        <div class="logo d-none d-sm-block">
                            <a href="/index"><img src="img/logo.png" alt=""></a>
                        </div>
                        <div class="logo d-none-block d-sm-none">
                            <a href="/index"><img src="img/logo1.png" alt=""></a>
                        </div>
                    </div>
                    <div class="col-md-9">
                        <nav class="main-menu mobile-menu">
                            <ul>
                                <li><a href="/index">Home</a></li>
                                <li><a href="/about-us">About us</a></li>
                                <li><a href="/classes">Courses</a></li>
                                <li><a href="/contact">Contact</a></li>
                                <li><a class="active" href="/tools">Tools</a></li>
                                <li class="phone-num"><i class="fa fa-phone"></i><span>+977 9855022260</span></li>
                            </ul>
                        </nav>
                        <div id="mobile-menu-wrap"></div>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- Header End -->

    <section class="elements-section spad">
        <div class="container">
            <!-- Icon Boxes -->
            <div class="icon-boxes">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="section-title">
                            <div class="row">
                                
                                <form action="/verify" method="POST">
                                    <!-- Product type	Brand Name	Discounted Price	Original Price	Short Description	Long Description	Specifications/Ingrediants	Usage	image name -->
                            
                                        
                                        <div class="form-group">
                                          <label for="exampleFormControlInput1">Certificate ID</label>
                                          <input  class="form-control" name="certi" required>
                                        </div> 
                                        
                                  <button type="submit" class="btn btn-primary">Submit</button>
                            
                                </form>

                        </div>
                            <div style="margin-top: 15px;" class="row">
                                <% if(user.length > 0){ %>
                                    <table class="table">
                                    <thead>
                                      <tr>
                                        <th scope="col">Name</th>
                                        <th scope="col">Email-ID</th>
                                        <th scope="col">Certifcate-ID</th>
                                        <th scope="col">Course</th>
                                      </tr>
                                    </thead>
                                    <tbody>
                                    <%user.forEach(function(cat){ %>

                                      <tr>
                                        <td> <%=cat.name%></td>
                                          <!-- <td> <a ="/admin/categories/edit-category/<%=cat._id%>" >Edit </a></td>
                                          <td> <a class="confirmDeletion" href="/admin/categories/delte-category/<%=cat._id%>" >Delete </a></td> -->
                                          <td> <%=cat.email%></td>
                                          <td> <%=cat.certi_id%></td>
                                          <td> <%=cat.course%></td>
                                      </tr>
                                    <% }); %>
                                      
                                    </tbody>
                                  </table>
                                  <% }else{ %>

                                    <h3 class="text-danger">The Certificate ID you are looking for does not exist.</h3>
                                    <% } %>
                            </div>
                    </div>
                </div>
                <div style="margin-top: 70px;" class="row">
                    <div class="col-lg-4">
                        <div class="single-icon-boxes">
                            <i class="flaticon-004-dumbbell"></i>
                            <h3>Training Results</h3>
                            <p>BMI Calculator</p>
                            <p>Body Fat %</p>
                            <p>Daily Calorie Need</p>
                            <p>Physical Activity Calorie</p>
                            <p>Weight Training Load Finder</p>
                            <p>Waist Circumference</p>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="single-icon-boxes">
                            <i class="flaticon-003-tshirt"></i>
                            <h3>Responsible Trainer</h3>
                            <p>Blood Pressure Tool</p>
                            <p>Heart Attack Risk Assessment</p>
                            <p>Heart Rate Zone Calculator</p>
                            <p>Depression Assessment</p>
                            <p>Risk of Chronic Disease</p>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="single-icon-boxes">
                            <i class="flaticon-009-medal"></i>
                            <h3>Success Section</h3>
                            <p>Class Theory & References</p>
                            <p>Body Parts & Equipments Web Guide</p>
                            <p>Certification Verification Portal</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Footer Section Begin -->
    <footer class="footer-section">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="footer-text">
                        <div class="reserved">
                            Nepal Council of Exercise | 
                            Copyright &copy;<script>document.write(new Date().getFullYear());</script>
                        </div>
                        <div class="social-links">
                            <a href="https://www.facebook.com/Nep.CoE"><i class="fa fa-facebook"></i></a>
                            <a href="https://www.instagram.com/nep.coe"><i class="fa fa-instagram"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- Footer Section End -->
    <!-- Search model -->
    <div class="search-model">
        <div class="h-100 d-flex align-items-center justify-content-center">
            <div class="search-close-switch">+</div>
            <form class="search-model-form">
                <input type="text" id="search-input" placeholder="Search here.....">
            </form>
        </div>
    </div>
    <!-- Search model end -->
    
    <!-- Js Plugins -->
    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/jquery.slicknav.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/circle-progress.min.js"></script>
    <script src="js/main.js"></script>
</body>

</html>